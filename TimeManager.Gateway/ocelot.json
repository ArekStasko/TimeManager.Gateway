{
  "Routes": [
    {
      "UpstreamPathTemplate": "/data/{type}/{method}",
      "UpstreamHttpMethod": [ "POST", "DELETE" ],

      "DownstreamPathTemplate": "/api/{type}/{method}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "timemanager.data",
          "Port": 80
        }
      ],

      "RateLimitOptions": {
        "ClientWhitelist": [],
        "EnableRateLimiting": true,
        "Period": "1s",
        "PeriodTimespan": 1,
        "Limit": 1
      }
    },
    {
      "UpstreamPathTemplate": "/idp/{type}/{methodType}/{method}",
      "UpstreamHttpMethod": [ "POST" ],

      "DownstreamPathTemplate": "/api/{type}/{methodType}/{method}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "timemanager.idp",
          "Port": 80
        }
      ],

      "RateLimitOptions": {
        "ClientWhitelist": [],
        "EnableRateLimiting": true,
        "Period": "1s",
        "PeriodTimespan": 1,
        "Limit": 1
      }
    }
  ],
  "GlobalConfiguration": {}
}